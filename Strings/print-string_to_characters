#include<iostream>
using namespace std;
#include<bits/stdc++.h>
int main()
{
string str;
getline(cin,str);
int i=0;
while(i<str.length())
{
cout<<str[i]<<endl;
i++;
}
}
############################################################################################################################
Input:
I am good
###############################################################################################################################
Output:
I
 
a
m
 
g
o
o
d
#############################################################################################################################
